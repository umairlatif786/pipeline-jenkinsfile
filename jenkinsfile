
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building project..'
		sh 'make' 
                archiveArtifacts artifacts: '**/target/*.jar', fingerprint: true		echo 'building done..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing project..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying project....'
            }
        }
	stage('unwanted'){
	steps{
		echo 'Hello World!'
		}
	}
    }
}
